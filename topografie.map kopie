MAP

  #=============================================================================
  # MAP: algemeen
  #=============================================================================  
  
  NAME        "Topografie Amsterdam"
  EXTENT      100000 450000 150000 500000
  UNITS       meters
  MAXSIZE     4000
  
  # ONTWIKKEL
    FONTSET     "//swappams0498/geodata/fonts/fonts.list"
    SHAPEPATH   "//swappams0498/geodata/topografie"
    CONFIG      "PROJ_LIB" "D:/ms4w/proj/nad/"
  
  # ACCEPTATIE
    #FONTSET     "//win0143vm/geodata/fonts/fonts.list"
    #SHAPEPATH   "//win0143vm/geodata/topografie"
  
  # PRODUCTIE
    #FONTSET     "//win0139vm/geodata/fonts/fonts.list"
    #SHAPEPATH   "//win0139vm/geodata/topografie"



  #=============================================================================
  # MAP: algemeen uitvoer
  #=============================================================================    
  
  IMAGETYPE   png_afb
  SIZE        256 256
  
  WEB
    IMAGEPATH "/ms4w/tmp/ms_tmp/"
    IMAGEURL  "/ms_tmp/"
    METADATA

      "ows_enable_request"           "*"
      "ows_title"                    "Topografie Amsterdam"
      "ows_srs"                      "EPSG:28992 EPSG:54004 EPSG:4326 EPSG:3857"
      "ows_feature_info_mime_type"   "text/xml"
	  
	  # ONTWIKKEL
        "ows_onlineresource"         "http://127.0.0.1:8080/cgi-bin/topografie?"
	  
	  # ACCEPTATIE
        #"ows_onlineresource"         "http://acc.diva.asp4all.nl/cgi-bin/topografie?"
	  
	  # PRODUCTIE
        #"ows_onlineresource"         "http://www.diva.amsterdam.nl/cgi-bin/topografie?"

    END
    
  END
  
  PROJECTION
    "init=epsg:28992" 
  END
  

  #-------------------------------------------------------------------
  # OUTPUTFORMATs
  #   standaard zijn bekend: gif, png, png24, jpeg, wbmp, swf, GTiff
  #-------------------------------------------------------------------  

  OUTPUTFORMAT
   NAME         png_afb
   DRIVER       "AGG/PNG"
   MIMETYPE     "image/png"
   IMAGEMODE    RGB
   EXTENSION    "png"
   FORMATOPTION "INTERLACE=OFF"
   FORMATOPTION "QUANTIZE_FORCE=ON"
   #FORMATOPTION "QUANTIZE_NEW=ON"
   #FORMATOPTION "PALETTE_FORCE=ON"
  END

  OUTPUTFORMAT
   NAME         gif_afb
   DRIVER       "GD/GIF"
   MIMETYPE     "image/gif"
   IMAGEMODE    PC256
   EXTENSION    "gif"
   FORMATOPTION "INTERLACE=OFF"
  END
  


  
 
